/* 
##########################################
     need to add other patient stuff
##########################################
*/
public class Patient implements Comparable<Patient> {
	//Information about patient 
	private int id;
	private String Name;
	private String StreetName;
	private String City;
	private String Zip;
	private String Phone;
	private String Injury;
	private int Priority;
	
	public Patient(int i, String n, String sN, String c, String z, String p, String inj, int prior){
		id = i;
		Name = n;
		StreetName = sN;
		City = c;
		Zip = z;
		Phone = p;
		Injury = inj;
		Priority = prior;
	}
	// ID
	public int getID(){
		return id;
	}
	//Name
	public String getName(){
		return Name;
	}
	//Street
	public String getStreetName(){
		return StreetName;
	}
	//City
	public String getCity(){
		return City;
	}
	//Zip
	public String getZip(){
		return Zip;
	}
	//Phone # 
	public String getPhone(){
		return Phone;
	}
	//Injury
	public String getInjury(){
		return Injury;
	}
	//Priority
	public int getPriority(){
		return Priority;
	}
	
	//Comparator
	public boolean equals(Patient other){
		return this.getPriority() == other.getPriority();
	}
	
	//Compares patients and prioritizes
	// this should queue them according to Int Priority (Hardcoded for now) 
	// 1 being main priority, 2 being queued, then so on and so forth
	public int compareTo(Patient other){
		if(this.equals(other))
			return 0;
		else if(getPriority() > other.getPriority())
			return 1;
		else
			return -1;
	}
	//Prints out information
	//Why isn't this printing when program executes??
	public String toString(){
		return ("ID: " + getID() + "Name: " + getName() + "Address: " + getStreetName() + "City: " + getCity() + "Zip: " + getZip() + "Phone: " + getPhone()
		+ "Injury: " + getInjury() + "Priority: " + getPriority());
	}
	
	
	
}
